#include <stdio.h>

void main(){
    int arr1[3][4] ={ {4, 5, 9, 47}
                    , {45, 87, 61, 65}
                    , {21, 4, 9, 987}
                    };

        for(int i=0;i<3;i++){
                printf("Row %d: ",i);
            for(int j=0;j<4;j++){
                printf("%d ",arr1[i][j]);
            }
            printf("\n");
        }


    getch();
}
