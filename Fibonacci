//Fibonacce series
#include <stdio.h>

void main(){
    int num[100];
    int n,fib;
    printf("how many fibonacce number will show:");
    scanf("%d",&n);
num[0] =0;
num[1] =1;
    for(int i=2;i<n;i++){
        num[i] = num[i-1] + num[i-2];
    }


    for(int i=0;i<n;i++){
        printf("%d ",num[i]);
    }
    getch();
}
