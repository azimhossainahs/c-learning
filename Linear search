#include <stdio.h>

void main(){
    int myNum[] = {15, 56, 78, 65, 32, 12, 13, 11, 9, 1};
    int n,length,pos = -1;
    printf("Enter a number to find if it is already exist in array :");
    scanf("%d",&n);

    length = sizeof(myNum) / sizeof(myNum[0]);


    for(int i=0;i<length;i++){
        if(n == myNum[i]){
            pos=i+1;
            break;
        }
    }
if(pos == -1){
    printf("The number you entered Number DoesnÂ´t Exist\n------------------------------------------Thank You------------------------------------------");
}else{
printf("***************Wow You Entered Number is Available*******************\n\tThe position of the number %d is at %d",n ,pos);}


    getch();
}
